    <ActionBlock>
      <Name value="AddMovie" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[/*-- ADD/EDIT Switcher --*/
if form.addMovie.addMovieCaption = "Add movie" then

	/*-- Prepare item for add --*/
	var newMovie = form.mList.addItem()
		newMovie.id = globals.selectedId
		newMovie.name = form.addMovie.name

		if isTrue(form.seriesToggler) then
			newMovie.type = "series"
		else
			newMovie.type = "movie"
		endif

		newMovie.yearOfRelease = form.addMovie.yearOfRelease
		newMovie.description = form.addMovie.description
		newMovie.starring.fromJson(form.addMovie.starring.toJson())
		newMovie.rating.fromJson(form.addMovie.rating.toJson())
		newMovie.genres.fromJson(form.addMovie.genres.toJson())

	/*-- Send Json data to server --*/
	var addJson = newMovie.toJson()
	system.console(ToString(addJson))
	var response = apiobjects.videoteka.postAsync("create", addJson)
	
	if (response.isError()) then
		system.consoleError(response.getErrorMessage())
		system.alert("Couldn't add record, please try again later")
		form.statusCode = ToString(response.getStatusCode())
	else

		form.statusCode = "Hooray! Your " + newMovie.type + " " + newMovie.name + " was succesfully add!"		/*-- Clear input fields --*/
		form.statusCode = form.addMovie.name + " was added to your collection"
		form.addMovie.name.clear()
		form.addMovie.addType.clear()
		form.addMovie.yearOfRelease.clear()
		form.addMovie.description.clear()
		form.addMovie.starring.clear()
		form.addMovie.rating.clear()
		form.addMovie.genres.clear()
	endif
	
	/*-- Clear editing variable --*/
	form.editingWithoutSave = false
endif

if form.addMovie.addMovieCaption = "Update movie" then
	
	/*-- Prepare item for updating --*/
	var newMovie = form.mList.addItem()
		newMovie.id = globals.selectedId
		newMovie.name = form.addMovie.name

		if isTrue(form.seriesToggler) then
			newMovie.type = "series"
		else
			newMovie.type = "movie"
		endif

		newMovie.yearOfRelease = form.addMovie.yearOfRelease
		newMovie.description = form.addMovie.description
		newMovie.starring.fromJson(form.addMovie.starring.toJson())
		newMovie.rating.fromJson(form.addMovie.rating.toJson())
		newMovie.genres.fromJson(form.addMovie.genres.toJson())

	/*-- Remove updating row, so it can be add again and wont be there twice --*/
	form.mList.removeItem(globals.selectedRowId)

	/*-- Update table and server with Json data --*/
	var addJson = newMovie.toJson()
	system.console(ToString(addJson))

	var response = apiobjects.videoteka.postAsync("update", addJson)	
	if (response.isError()) then
		system.consoleError(response.getErrorMessage())
		system.alert("Couldn't update record " + globals.selectedId + ", please try again later")
		form.statusCode = ToString(response.getStatusCode())
	else
		form.statusCode = "Hooray! Your " + newMovie.type + " " + newMovie.name + " was succesfully edited!"
	endif

	form.editingWithoutSave = false
endif
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_button1_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[system.console("Showing details for row" + params.data.id)
form.isLoading = true

globals.selectedId = params.data.id
globals.selectedRowId= params.data.index()
form.addMovie.addMovieEditor = true
form.editingWithoutSave = true

var response = apiobjects.videoteka.getAsync("record?id=" + params.data.id)

if (response.isError()) then
	system.consoleError(response.getErrorMessage())
	form.statusCode = ToString(response.getStatusCode())
else
	form.addMovie.addMovieCaption = "Update movie"	
	form.addMovie.fromJson(response.getJsonString())

	/*-- Clear datalists if none records exists --*/
	if isFalse(hasValue(response.extractJson("starring"))) then
		form.addMovie.starring.clear()	
	endif

	if isFalse(hasValue(response.extractJson("rating"))) then
		form.addMovie.rating.clear()	
	endif

	if isFalse(hasValue(response.extractJson("genres"))) then
		form.addMovie.genres.clear()	
	endif

	if params.data.type = "series" then
		form.seriesToggler = true
		form.seriesTogglerWidth = 33
	else
		form.seriesToggler = false
		form.seriesTogglerWidth = 50
	endif
	
	form.statusCode = ToString(response.getJsonString())
	system.console(ToString(response.getJsonString()))

	form.apiobjects.list.fillFromJson(response.getJsonString())


	form.isLoading = false

endif]]></Script>
