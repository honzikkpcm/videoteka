    <ActionBlock>
      <Name value="handler_delMovieBtn_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var canIRemoveMovie = system.confirm("Do you really want remove row " + params.data.name)

if canIRemoveMovie then
	var response = apiobjects.videoteka.postAsync("delete", "{\"id\":\"" + params.data.id + "\"}")
	
	if (response.isError()) then
		system.consoleError(response.getErrorMessage())
		form.statusCode = ToString(response.getStatusCode())
	else
		system.console("Removing item " + params.data.name)
		form.statusCode = params.data.name + " was removed from your collection!"
		system.console(ToString(response.getJsonString()))		
		params.data.removeMe()
	endif


endif	]]></Script>
      </ActionLangBody>
