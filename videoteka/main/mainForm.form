  <Actions>
    <ActionBlock>
      <Name value="LoadMovies" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[system.console("Loading movies")

var response = apiobjects.videoteka.getAsync("list")

form.declListQueryColumns = "id, name, type"
form.resultColumn = "id"
form.declListQueryFilter = "type eq 'movie'"

var response2 = form.apiobjects.declListQuery1.getResultAsync(form.declListQueryColumns, form.declListQueryFilter)


if globals.listState = defaultString() then
	// set defaults
	form.apiobjects.listCtrl.setSorting("name:desc")
	form.apiobjects.listCtrl.focusFirstRecord()
else
	system.console("Loading serialized data")
	form.apiobjects.listCtrl.loadState(globals.listState)
	system.console("Loaded saved state::" + globals.listState)
endif


form.apiobjects.listCtrl.setReady(true)


if (response.isError()) then
	system.consoleError(response.getErrorMessage())
	form.statusCode = ToString(response.getStatusCode())
else
	
	form.addMovie.addMovieCaption = "Add movie"
	form.mList.fromJson(response.getJsonString())
	form.apiobjects.list.fillFromJson(response.getJsonString())

	form.statusCode = ToString(response.getJsonString())
	system.console(ToString(response.getJsonString()))	
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="ShowDetails" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[system.console("showing detail at row ")

navigation.main.detailFrm.go()]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="AddMovie" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if form.addMovie.addMovieCaption = "Add movie" then
	system.console("Adding item")
	
	var newMovie = form.mList.addItem()
		//newMovie.id = subString(replaceString(newGuid(), "-", ""), 1, 9)
		newMovie.name = form.addMovie.addName
	
		if isTrue(form.seriesToggler) then
			newMovie.type = "series"
		else
			newMovie.type = "movie"
		endif
		
		newMovie.yearOfRelease = form.addMovie.addRelease
		newMovie.description = form.addMovie.addDescription
		//newMovie.starring = form.actorsTemp.toJson()
		//newMovie.starring = form.addMovie.addStaring.toJson()		

	var addJson = newMovie.toJson()
	system.console(ToString(addJson))
	var response = apiobjects.videoteka.postAsync("create", addJson)
	
	if (response.isError()) then
		system.consoleError(response.getErrorMessage())
		system.alert("Couldn't add record, please try again later")
		form.statusCode = ToString(response.getStatusCode())
	else
		form.statusCode = form.addMovie.addName + " was added to your collection"
		form.addMovie.addName.clear()
		form.addMovie.addType.clear()
		form.addMovie.addRelease.clear()
		form.addMovie.addDescription.clear()
	endif
endif

if form.addMovie.addMovieCaption = "Update movie" then
	system.console("Updating item " + globals.selectedId)
	form.mList.removeItem(globals.selectedRowId)
	var newMovie = form.mList.addItem()
		newMovie.id = globals.selectedId
		newMovie.name = form.addMovie.addName
		newMovie.type = form.addMovie.addType
		newMovie.yearOfRelease = form.addMovie.addRelease
		newMovie.description = form.addMovie.addDescription
		//newMovie.starring = form.addMovie.addStaring.toJson()	
	var addJson = newMovie.toJson()
	system.console(ToString(addJson))
	var response = apiobjects.videoteka.postAsync("update", addJson)
	if (response.isError()) then
		system.consoleError(response.getErrorMessage())
		system.alert("Couldn't update record " + globals.selectedId + ", please try again later")
		form.statusCode = ToString(response.getStatusCode())
	endif
endif
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_button1_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[system.console("Showing details for row" + params.data.id)
globals.selectedId = params.data.id
globals.selectedRowId= params.data.index()
form.addMovie.addMovieEditor = true

var response = apiobjects.videoteka.getAsync("record?id=" + params.data.id)

if (response.isError()) then
	system.consoleError(response.getErrorMessage())
	form.statusCode = ToString(response.getStatusCode())
else
	form.addMovie.addMovieCaption = "Update movie"
	form.addMovie.addName = replaceString(response.extractJson("name"), "\"", "")
	//form.addMovie.addRelease = response.extractJson("yearOfRelease")

	if params.data.type = "series" then
		form.seriesToggler.setTouched(true)
	else
		form.seriesToggler.setTouched(false)	
	endif
	
	form.addMovie.addType = replaceString(response.extractJson("type"), "\"", "")
	form.addMovie.addDescription = replaceString(response.extractJson("description"), "\"", "")
	form.statusCode = ToString(response.getJsonString())
	system.console(ToString(response.getJsonString()))
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
